	<script>
		var fenxiangurl, fengxianglog, fengxiangtext, fengxiangtop;
		angular.module('jobdetailCtr', ["myApp"])
			.controller('myCtrl', function($scope, $http, $data) {
				var deceleration = mui.os.ios ? 0.003 : 0.0009;
				mui('.mui-scroll-wrapper').scroll({
					bounce: true,
					indicators: true, //是否显示滚动条
					deceleration: deceleration,
					scrollX: true, //是否横向滚动
				});
				var jiazai = document.querySelector('.jiazai');
				setTimeout(function() {
					jiazai.style.display = 'none';
				}, 400)
				var jid = localStorage.getItem('jid');
				var tagarr = [];
				$scope.qyimg = true;

				window.addEventListener('job', function(e) {
					//在父页面中添加监听事件，刷新页面

					$data.dengl().success(function(dl) {
						//					console.log(JSON.parse(dl));
						$scope.dl = dl;
					});
					xuanranyemian();

				});

				xuanranyemian();
				$scope.cgtel = function() {
					$scope.isShow = true;
				}
				$scope.zt = function() {
					$scope.isShow = false;
				}

				//tzdl
				$scope.tzdl = function() {
					mui.openWindow({
						url: '../login/login.html',
						id: 'login.html',
					})
				}
				//公司详情
				$scope.comdetail = function() {

					mui.openWindow({
						url: '../company/company.html',
						id: 'company',
					})
				}
				//map
				$scope.Tomap = function() {

					mui.openWindow({
						url: 'map.html',
						id: 'map.html',
						extras: {

						},
						createNew: false, //是否重复创建同样id的webview，默认为false:不重复创建，直接显示
						waiting: {
							autoShow: true, //自动显示等待框，默认为true
							title: '正在加载...', //等待对话框上显示的提示内容
						}
					})
				}
				$scope.jobtomp = function() {
					console.log(this.x.jid);
					localStorage.setItem('jid', this.x.jid);
					mui.openWindow({
						url: 'job.html',
						id: 'job.html',
						extras: {

						},
						createNew: true, //是否重复创建同样id的webview，默认为false:不重复创建，直接显示
						waiting: {
							autoShow: true, //自动显示等待框，默认为true
							title: '正在加载...', //等待对话框上显示的提示内容
						}
					})
				}

				//				console.log(jid);
				$data.dengl().success(function(dl) {
					//					console.log(JSON.parse(dl));
					$scope.dl = dl;
				});

				function xuanranyemian() {
					$data.jobdetail(jid).success(function(data) {
						//						console.log(JSON.stringify(data.data));
						fengxiangtop = "直招帮";
						fengxianglog = $$$.imgurl + 'data/weixin/1.png';
						fengxiangtext = "【直招帮】 " + data.data.jobs_name + "正在招" + data.data.category_cn +
							"综合工资" +
							data.data.minwage +
							"--" +
							data.data.maxwage +
							" 就等你来报名";
						//console.log(fengxiangtext);
						fengxiangtop = fengxiangtext;
						//map
						localStorage.setItem('addresstocompor', data.data.address);
						localStorage.setItem('xmap', data.data.map_x);
						localStorage.setItem('ymap', data.data.map_y);
						localStorage.setItem('comp', data.data.companyname);
						//公司详情
						localStorage.setItem("cid", data.data.cid);
						localStorage.setItem("jid", data.data.jid);
						//渲染
						$scope.qyimg = false;
						$scope.data = data.data;
						$scope.img = data.img;
						$scope.job = data.job;
						$scope.imgurl = $$$.imgurl;
						var contents = data.data.contents
						var obj = Resoveto(contents);
						$scope.obj = obj;
						$scope.value = obj.value;
						$scope.isShow = false;
						$scope.xinyu = $$$.imgurl + 'data/app/xinyu.png';
						//						console.log(JSON.stringify(obj.value));
						//						$scope.tag=data.job.tag_cn;
						for(var i = 0; i < data.job.length; i++) {
							var tags = data.job[i].tag_cn.split(',');

							tagarr.push(tags);
						}

						$scope.tag = tagarr;

						$scope.contantshow = data.data.contantshow;
					});
				}
				//dianhua

				function Resoveto(data) {
					//console.log(data);
					var arry = [];
					var ay = new Array();
					var to = new Array();
					ay = data.split('【')
					for(var i = 0; i < ay.length; i++) {
						to = ay[i].split('】');

						var st = String(to[1]);
						///\d、/
						var stin = st.replace(/\d、/g, '|');
						var stin1 = stin.replace(/\&nbsp;/g, '');
						var xiangqing = stin1.split('|');

						if(xiangqing.length > 1) {
							var au = [];
							for(var x = 1; x < xiangqing.length; x++) {
								au.push(xiangqing[x]);
							}
							//console.log(au);
							var ob = {
								title: to[0],
								text: au,
								isbul: 1
							};
						} else {
							var ob = {
								title: to[0],
								text: to[1],
								isbul: 2
							};
						}

						arry.push(ob);
					}

					arry.shift(0)
					if(arry.length) {
						return {
							key: 1,
							value: arry
						};
					} else {
						return {
							key: 2,
							value: data
						};
					}
				}
			})
		//判断是否收藏
		mui.ajax($$$.siturl + 'm=App&c=Tool&a=islogin', {
			type: 'get',
			dataType: 'json',
			success: function(data) {
				//已经登陆
				var isjid = localStorage.getItem('jid');
				if(data == 1) {
					mui.ajax($$$.siturl + 'm=App&c=ApplyJob&a=checkfavor', {
						type: 'post',
						data: {
							jid: isjid
						},
						success: function(isShouCang) {
							if(isShouCang == 1) {
								var result = "";
								result += '<img src="images/175.png" width="31.2px"><br><span style=" color: #666666;">已收藏</span>';
								var showcang = document.querySelector('#showcang');
								showcang.innerHTML = result;
							}
						}

					});
				}
			},
			error: function(err) {
				//alert(err);
			}
		});
		//收藏职位
		// 点击收藏实现图标变色并且申请职位
		var shoucang_icon = document.querySelector('#showcang');
		shoucang_icon.addEventListener('touchstart', function() {
			shoucang();
		});

		function shoucang() {

			var jid = localStorage.getItem('jid');

			mui.ajax($$$.siturl + 'm=App&c=ApplyJob&a=resume_showcang', {
				type: 'post',
				data: {
					jid: jid
				},
				dataType: 'json',
				success: function(data) {
					if(data.staus == 9) {
						mui.openWindow({
							url: '../login/bianji_delete.html',
							id: 'bianji_delete'
						});
						return;
					}

					if(data.staus == 1) {
						mui.ajax($$$.siturl + 'm=App&c=ApplyJob&a=checkfavor', {
							type: 'post',
							data: {
								jid: jid
							},
							success: function(isShouCang) {
								if(isShouCang == 1) {
									var result = "";
									result += '<img src="images/175.png" width="31.2px"><br><span style=" color: #666666;">已收藏</span>';
									var showcang = document.querySelector('#showcang');
									showcang.innerHTML = result;
								} else {
									var result = "";
									result += '<img src="images/131.png" width="31.2px"><br><span style=" color: #666666;">收藏职位</span>';
									var showcang = document.querySelector('#showcang');
									showcang.innerHTML = result;
								}
							}

						});
						mui.toast(data.msg, {
							duration: 'long',
							type: 'div'
						})
					}
					//没有登录
					if(data.staus == 0) {
						mui.toast(data.msg, {
							duration: 'long',
							type: 'div'
						})
						mui.openWindow({
							url: '../login/login.html',
							id: 'bianji_delete',
						})
						//     shoucang_icon.classList.remove('active');
