<!DOCTYPE html>
<html ng-app="wodeCtr">

	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
		<title></title>

		<link href="css/mui.min.css" rel="stylesheet" />

		<style type="text/css">
			li {
				list-style: none;
			}
			
			.staricon {
				font-size: 15px;
				height: 15px;
				line-height: 15px;
				color: #DF6659;
			}
			
			html,
			body {
				width: 100%;
				height: 100%;
			}
			
			@font-face {
				font-family: 'lanze';
				src: url('lib/fonts/icomoon.eot') format('embedded-opentype'), url('lib/fonts/icomoon.svg') format('svg'), url('lib/fonts/icomoon.ttf') format('truetype'), url('lib/fonts/icomoon.woff') format('woff');
			}
			
			[class^="icon-"],
			[class*=" icon-"] {
				font-family: lanze;
				font-style: normal;
			}
			
			.icon-mobilephone:before {
				content: "\e908";
				font-size: 13px;
			}
			/*头部栏颜色*/
			
			.header-top {
				/*background-color: #5CA8FC;*/
				opacity: 1;
			}
			
			#circle .mui-table-view-cell {
				padding-top: 0;
				padding-bottom: 0;
				background-color: #fff;
				border: none;
			}
			
			#circle .mui-table-view-cell span {
				display: inline-block;
				width: 2.4em;
				height: 2.3em;
				background-image: url(images/index_circle.png);
				background-size: 200% 200%;
				background-repeat: no-repeat;
			}
			
			.kuaisu {
				background-position: left bottom;
			}
			
			.fujin {
				background-position: left top;
			}
			
			.zixun {
				background-position: right bottom;
			}
			
			.mingqi {
				background-position: right top;
			}
			/*筛选框*/
			
			#selector span {
				display: inline-block;
				border: 1px solid #737373;
				border-radius: 5px;
				margin-right: 9px;
				margin-bottom: 14px;
				height: 20px;
				line-height: 10px;
				padding: 6px 12px;
				box-sizing: border-box;
				font-size: 10px;
				overflow: hidden;
			}
			
			#selected {
				padding: 12px 23px 14px 32px;
			}
			/*消息面板*/
			
			.news-tab {
				height: 70px;
				background-color: #fff;
			}
			
			.news-tab .left {
				width: 70px;
				height: 100%;
				float: left;
				box-sizing: border-box;
				padding: 12px 10px 12px 13px;
			}
			
			.news-tab .left img {
				width: 100%;
				height: 100%;
			}
			
			.news-tab .right {
				width: 70%;
				height: 100%;
				float: left;
				padding: 16px 13px 16px 0;
			}
			
			.news-tab .right .line-one {
				font-size: 14px;
				line-height: 14px;
				height: 14px;
				margin-bottom: 12px;
				color: #323232;
				position: relative;
			}
			
			.news-tab .right .line-one span {
				position: absolute;
				right: 0;
				top: 0;
				color: #5CA8FC;
				font-size: 14px;
			}
			
			.news-tab .right .line-two {
				font-size: 12px;
				line-height: 12px;
				height: 12px;
				color: #737373;
			}
			/*设置面板*/
			
			#tabbar-with-my .title {
				/*display: inline-block;*/
				position: relative;
				height: 180px;
				/*background-color:#fff;*/
				background-repeat: no-repeat;
				background-size: 100% 139px;
				padding: 40px 13px 0 11px;
				overflow: hidden;
				text-align: center;
			}
			
			#tabbar-with-my .title .left {
				/*width:17.3%;*/
				width: 50px;
				float: left;
			}
			
			#tabbar-with-my .title .left img {
				width: 100%;
				height: 100%;
			}
			
			#tabbar-with-my .title .right {
				color: #fff;
				position: relative;
				color: #000;
				font-size: 17px;
				text-align: center;
			}
			
			#tabbar-with-my .title .right .line-one {}
			
			#tabbar-with-my .title .right .line-two {
				font-size: 11px;
				height: 11px;
				line-height: 11px;
				/*display: none;*/
			}
			
			.arrow {
				position: absolute;
				right: 3px;
				top: 50%;
				margin-top: -6px;
				width: 9px;
				height: 20px;
				overflow: hidden;
			}
			
			.arrow::after {
				content: ' ';
				position: absolute;
				right: 3px;
				top: 0;
				width: 12px;
				height: 12px;
				border: 3px solid #C4CDD9;
				transform: rotate(45deg);
			}
			/*已申请职位，收藏。。。*/
			
			#tabbar-with-my .news-content .block {
				width: 33.3%;
				float: left;
				margin: 13px 0;
				font-size: 12px;
				line-height: 12px;
			}
			
			#tabbar-with-my .news-content .block+div {
				border-left: 1px solid #cdcdcd;
			}
			
			#tabbar-with-my .news-content .block .img-t {
				position: relative;
				left: 50%;
				font-size: 20px;
				color: #fff;
				width: 20px;
			}
			
			#tabbar-with-my .news-content .block .text-b {
				margin-top: 9px;
				/*width: 100%;*/
				text-align: center;
				color: #fff;
				font-size: 15px;
				border-right: 1px solid #fff;
				margin-left: 5px;
				z-index: 100;
			}
			/*客服热线，招聘合作*/
			/*#tabbar-with-my .news-service .news-service-item {
				width: 100%;
				height: 50px;
				background-color: #fff;
			}
			*/
			/*#tabbar-with-my .news-service .news-service-item .left1 {
				box-sizing: border-box;
				padding: 17px 8px 0 10px;
				height: 50px;
				width: 40px;
				float: left;
			}
			
			#tabbar-with-my .news-service .news-service-item .left1 img {
				width: 100%;
			}
			
			#tabbar-with-my .news-service .news-service-item .left2 {
				color: #000;
				font-size: 14px;
				line-height: 11px;
				float: left;
				padding: 24px 0;
				font-family: "微软雅黑";
			}*/
			/*顶部栏颜色*/
			/*header {*/
			/*background-color: #5CA8FC !important;	*/
			/*}*/
			/*下拉框列表项小标签格式*/
			
			#sel_box div {
				display: inline-block;
				font-size: 10px;
				height: 24px;
				line-height: 20px;
				box-sizing: border-box;
				padding: 4px 12px;
				background-color: #F5F4F4;
				border-radius: 3px;
				margin-bottom: 13px;
				margin-right: 9px;
				border: 1px solid #737373;
			}
			
			.btitle {
				background-image: url("images/wode.png");
				background-size: 100% 100%;
				/*background:#fff;*/
				height: 270px;
			}
			
			.news-service-item {
				width: 100%;
				height: 50px;
				background-color: #fff;
			}
			
			.left1 {
				width: 50px;
				height: 50px;
				display: block;
				float: left;
			}
			
			.kefu {
				display: block;
				float: left;
				height: 50px;
				line-height: 50px;
				margin-left: 10px;
				font-size: 16px;
				color: #000;
			}
		</style>

	</head>

	<body ng-controller="myCtrl">
		<!--我的-->
		<div id="tabbar-with-my" class="" style="background-color: #efefef;">
			<!--顶部区域-->
			<div id="cjs"></div>
			<div class="btitle">

				<div id="title" class="title">
					<span style="position:absolute;top:30px;right:20px;"><img style="width:25px;height:25px;" src="images/shezhi.png" alt="" /></span>

					<div class="head">

						<div class="right" style="padding-left: 0;width:100%;">
							<span style="height:90px;width:90px;display:inline-block;border-radius:50%;background:#7fc46f;">
								<span style="height:80px;width:80px;display:inline-block;border-radius:50%;background:#afdc98;margin-top:5px;position:relative;">
									<img  src='app/community/images/comtx.png' alt="" style="width:75px;height:75px;position:absolute;top:2.5px;left:2.5px;">
								</span>

							</span>
							<div style=" color:#fff;font-size:18px;font-weight:bold;width:100%;display:block;padding:5px 10px;" ng-bind='data.userdata.realname'></div>
							<div style=" color:#fff;font-size:30px;width:100%;display:block;padding:0px 10px;">{{data.username}}</div>
						</div>

					</div>
				</div>
				<!--已申请职位，收藏，社区动态-->
				<div class="news-content" style="overflow: hidden;display: block;">
					<a href="app/apply/apply.html" style="color:#000;">
						<div class="block">
							<div class="img-t" ng-bind="sqdata.appcount">
								1
							</div>
							<div class="text-b">
								已申请职位
							</div>
						</div>
					</a>
					<a href="app/shoucang/shoucang.html" style="color:#000;">
						<div class="block">
							<div class="img-t" ng-bind="sqdata.favcount">
								2
							</div>
							<div class="text-b">
								收 藏
							</div>
						</div>
					</a>
					<a href="app/community/myTop.html" style="color:#000;">
						<div class="block">
							<div class="img-t" ng-bind="sqdata.displaycount">
								3
							</div>
							<div class="text-b" style="border-right:0px;">
								社区动态
							</div>
						</div>
					</a>
				</div>
			</div>

			<!--客服热线，招聘合作-->
			<div class="news-service">
				<!--客服热线，调起本地的电话-->
				<a href="tel:4008 334 588" style="display: block;position:relative;">
					<div class="news-service-item" style="position:relative;">
						<div class="left1" style="background:url(images/xlogo.png) 3px 6px no-repeat;background-size:100% 650%;">

						</div>
						<div class="left2">
							<div class="kefu">客服热线</div>
							<span style="display:block;color:#26aa12;line-height:50px;float:right;margin-right:50px;">4008-334-588</span>
							<span class="mui-navigate-right" style="position:absolute;top:50%;right:10px;"></span>

						</div>
					</div>
				</a>

				<div class="news-service-item zphz" style="margin-top:5px;border-bottom:1px solid #C0C0C0;position:relative;">
					<span class="left1" style="background:url(images/xlogo.png) 3px -56px no-repeat;background-size:100% 600%;">
							
						</span>
					<div class="left2">
						<div class="kefu">招聘合作</div>
						<span class="mui-navigate-right" style="position:absolute;top:50%;right:10px;"></span>
					</div>
				</div>

				<div class="news-service-item tjrz" style="position:relative;">
					<span class="left1" style="background:url(images/xlogo.png) 3px -99px no-repeat;background-size:100% 600%;">
							
						</span>
					<div class="left2 ">
						<div class="kefu">推荐入职</div>
						<span style="font-size:16px;color:#999999;position:absolute;top:14px;right:70px;display: inline-block;">抢！入职奖励<img src="images/jiangli.png" alt=""style="width:30px;height:30px;position:absolute;top:-7px;" /></span><span class="mui-navigate-right" style="position:absolute;top:50%;right:10px;"></span>
					</div>
				</div>
				<div class="news-service-item qd" style="margin-top:5px;border-bottom:1px solid #C0C0C0;position:relative;">
					<span class="left1" style="background:url(images/xlogo.png) 3px -163px no-repeat;background-size:100% 620%;">
							
						</span>
					<div class="left2 ">
						<div class="kefu">签到有礼</div>
						<span style="font-size:16px;color:#999999;position:absolute;top:14px;right:70px;display: inline-block;">赢好礼<img src="images/qdyl.png" alt=""style="width:30px;height:30px;position:absolute;top:-7px;" /></span><span class="mui-navigate-right" style="position:absolute;top:50%;right:10px;"></span>

					</div>
				</div>

				<div class="news-service-item jifenchoujiang" style="border-bottom:1px solid #C0C0C0;position:relative;">
					<span class="left1" style="background:url(images/xlogo.png) 3px -204px no-repeat;background-size:100% 600%;">
							
						</span>
					<div class="left2 ">
						<div class="kefu">积分抽奖</div>
						<span class="mui-navigate-right" style="position:absolute;top:50%;right:10px;"></span>
					</div>
				</div>
				<div class="news-service-item tjff" style="position:relative;">
					<span class="left1" style="background:url(images/xlogo.png) 3px -251px no-repeat;background-size:100% 600%;">
							
						</span>
					<div class="left2 ">
						<div class="kefu">体检返费</div>

						<span style="font-size:16px;color:#999999;position:absolute;top:14px;right:70px;display: inline-block;"><img src="images/fanf.png" alt=""style="width:30px;height:30px;position:absolute;top:-7px;" /></span><span class="mui-navigate-right" style="position:absolute;top:50%;right:10px;"></span>

					</div>
				</div>
			</div>

			<!--设置-->
			<!--<div class="news-service" style="margin-top:8px;" id="setting">
				<div class="news-service-item" style="position:relative;">
					<div class="left1">
						<img src="images/shezhi2.png" alt="">
					</div>
					<div class="left2">
						设置<span class="mui-navigate-right" style="position:absolute;top:50%;right:10px;"></span>
					</div>
				</div>
			</div>-->
		</div>
		<script src="js/mui.min.js"></script>
		<script type="text/javascript">
			mui.init();
		</script>
		<script src="js/ag1.46.js" type="text/javascript" charset="utf-8"></script>
		<script src="js/factory.js" text/javascript " charset="utf-8 "></script>
	<script>
		angular.module('wodeCtr', ['myApp'])
			.controller('myCtrl',
				function($scope, $http, $data) {

					var login_uid = localStorage.getItem('user_name');
					var login_pass = localStorage.getItem('user_pass');
					mui.ajax($$$.siturl + 'm=App&c=Mumber&a=logoin', {
						type: 'post',
						dataType: 'json',
						data: {
							uname: login_uid,
							password: login_pass
						},
						success: function(data) {
							console.log(JSON.stringify(data.data));
							$scope.data = data.data;
							
						},
						error: function(xhr, err) {
							//											 mui.alert(xhr);
						}
					});
					mui.ajax($$$.siturl + 'm=App&c=MyAllTodis&a=index', {
						type: 'get',
						dataType: 'json',
						//								data:{
						//									uname:login_uid,
						//									password:login_pass
						//								},
						success: function(data) {
							console.log(JSON.stringify(data.data));
							$scope.sqdata = data.data;
							console.log($scope.sqdata.favcount);
						}
					})
				})
	</script>

	</body>

	
</html>