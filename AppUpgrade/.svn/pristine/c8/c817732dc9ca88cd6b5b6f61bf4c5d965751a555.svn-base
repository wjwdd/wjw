							$scope.imgurl = $$$.imgurl;
							for(var i = 0; i < data.jobs.length; i++) {
								var tags = data.jobs[i].tag_cn.split(',');
								qytagarr.push(tags);
							}

							$scope.qytag = qytagarr;

						});
					}
				})
			//判断是否收藏
