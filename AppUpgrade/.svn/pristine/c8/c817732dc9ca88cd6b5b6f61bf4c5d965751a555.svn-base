						if(data.staus == 2) {
							mui.alert(data.msg);

							//						mui.alert(data.msg);
							//mui.toast(data.msg,{ duration:'long', type:'div' })
						}
						if(data.staus == 0) {
							//mui.toast(data.msg,{ duration:'long', type:'div' }) 
							mui.openWindow({
								url: '../login/login.html',
								id: 'login.html',
							})

						}
					},
					error: function(err) {
						mui.alert(err);
					}
				})

			})
			button_apply.addEventListener('touchmove', function() {
				this.classList.remove('active');
