							}

							$scope.tag = tagarr;

							$scope.contantshow = data.data.contantshow;
							$scope.setStyle = function(y) {

								if(y == '年终奖') return 'tag1';

								else if(y == '工作餐')
									return 'tag2';
								else if(y == '五险一金')
									return 'tag3';
								else if(y == '长白班')
									return 'tag4';
								else if(y == '两班倒')
									return 'tag5';
								else if(y == '双休')
									return 'tag6';
								else if(y == '工作轻松')
									return 'tag3';
								else if(y == '单休')
									return 'tag4';
								else if(y == '恒温车间')
