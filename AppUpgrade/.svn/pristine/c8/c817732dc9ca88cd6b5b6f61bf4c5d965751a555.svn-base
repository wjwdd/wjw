								else if(y == '坐着上班')
									return 'tag5';
								else
									return 'tag7';

							};
						});
					}

					function Resoveto(data) {
						//console.log(data);
						var arry = [];
						var ay = new Array();
						var to = new Array();
						ay = data.split('【')
						for(var i = 0; i < ay.length; i++) {
							to = ay[i].split('】');

							var st = String(to[1]);
							///\d、/
							var stin = st.replace(/\d、/g, '|');
							var stin1 = stin.replace(/\&nbsp;/g, '');
							var xiangqing = stin1.split('|');

							if(xiangqing.length > 1) {
								var au = [];
								for(var x = 1; x < xiangqing.length; x++) {
									au.push(xiangqing[x]);
								}
								//console.log(au);
								var ob = {
									title: to[0],
									text: au,
									isbul: 1
								};
							} else {
								var ob = {
									title: to[0],
									text: to[1],
									isbul: 2
								};
							}

							arry.push(ob);
						}

						arry.shift(0)
						if(arry.length) {
							return {
								key: 1,
								value: arry
							};
						} else {
							return {
								key: 2,
								value: data
							};
						}
					}
					//企业详情
					var qytagarr = [];

					function qyxuanranyemian(qydetail) {
