							mui(".pdiqu").popover("hide");
							mui(".pzhiwei").popover("hide");
							mui(".pxinzi").popover("hide");
							mui(".pfuli").popover("hide");
						});
						mui(document.body).on('touchmove', '.msk', function() {
							allsxhuifu();
							mui(".pdiqu").popover("hide");
							mui(".pzhiwei").popover("hide");
							mui(".pxinzi").popover("hide");
							mui(".pfuli").popover("hide");
						});
						//筛选的点击事件
						mui(document.body).on('tap', '.sxbtn1', function() {
							page = 1;
							if(this.innerHTML == '不限') {
								$scope.sxquyu = '区域';
								city = $scope.city;
							} else {
								$scope.sxquyu = this.innerHTML;
								localStorage.setItem('sel_did', this.innerHTML);
								city = localStorage.getItem('sel_did');
								console.log(city);

							}
							xr(page, city, job, min, max, welfare);
							allsxhuifu();
							mui(".pdiqu").popover("hide");
						})
						mui(document.body).on('tap', '.sxbtn2', function() {

							//						console.log(this.x.categoryname);
							if(this.innerHTML == '不限') {
								$scope.sxzhiwei = '职位';
								job = '';
							} else {
								$scope.sxzhiwei = this.innerHTML;
								localStorage.setItem('sel_zid', this.innerHTML);
								job = localStorage.getItem('sel_zid');
								console.log(job);

							}
							xr(page, city, job, min, max, welfare);
							allsxhuifu();
							mui(".pzhiwei").popover("hide");
						})
						mui(document.body).on('tap', '.sxbtn3', function() {

							//						console.log(this.x.categoryname);
							if(this.innerHTML == '不限') {
								$scope.sxxinzi = '薪资';
								min = '';
								max = '';
							} else {
								$scope.sxxinzi = this.innerHTML;
								localStorage.setItem('sel_xid', this.innerHTML);
								var regxid = localStorage.getItem('sel_xid');
								var re = /([0-9]+)([0-9]+)/g;
								min = regxid.match(re)[0];
								max = regxid.match(re)[1];

							}
							xr(page, city, job, min, max, welfare);

							allsxhuifu();
							mui(".pxinzi").popover("hide");
						})
						mui(document.body).on('tap', '.sxbtn4', function() {

							//						console.log(this.x.categoryname);
							if(this.innerHTML == '不限') {
								$scope.sxfuli = '福利';
								welfare = '';
							} else {
								$scope.sxfuli = this.innerHTML;
								localStorage.setItem('sel_fid', this.innerHTML);
								welfare = localStorage.getItem('sel_fid');

							}
							xr(page, city, job, min, max, welfare);

							allsxhuifu();
							mui(".pfuli").popover("hide");
						})

						function sxhuifu(a) {
							for(var i = 0; i < 4; i++) {
								//								sxup[i].children[1].className = "mui-icon mui-icon-arrowdown";
								if(sxup[i] == a) {
									if(a.children[1].className == "mui-icon mui-icon-arrowdown") {
										//								alert(this.children[1].className);
										a.children[1].className = "mui-icon mui-icon-arrowup"
									} else if(a.children[1].className == "mui-icon mui-icon-arrowup") {

										a.children[1].className = "mui-icon mui-icon-arrowdown"
									}
								} else {
									sxup[i].children[1].className = "mui-icon mui-icon-arrowdown"
								}
							}
						}

						function allsxhuifu() {
							for(var i = 0; i < 4; i++) {

								sxup[i].children[1].className = "mui-icon mui-icon-arrowdown"

							}
						}
						//轮播图
						$data.indexlunbo().success(function(response) {
							//							console.log(JSON.stringify(response));
							$scope.lunbo = response.data;
							//														console.log(JSON.stringify($scope.lunbo.siled[1]));
							//						$scope.tag=response.data.tag;
							//						console.log(response.data.tag);
							//						console.log($scope.classname2)
						});
						mui.plusReady(function() {
							//获得slider插件对象
							var gallery = mui('.mui-slider');
							gallery.slider({
								interval: 2000 //自动轮播周期，若为0则不自动播放，默认为0；
							});
						});
					})
			//关闭启动页
			mui.plusReady(function() {
				plus.navigator.closeSplashscreen();
			})
		</script>
	</body>

</html>