								var indexre = plus.webview.getWebviewById('index1.html');
								var comre = plus.webview.getWebviewById('community.html');
								var xiaoxire = plus.webview.getWebviewById('xiaoxi.html');
								var wodere = plus.webview.getWebviewById('wode.html');
								switch(nt) {　　　　
									case plus.networkinfo.CONNECTION_ETHERNET:
										　　　　
									case plus.networkinfo.CONNECTION_WIFI:
										wifi(indexre, wodere, xiaoxire, comre);　　　
										break;　　　　
									case plus.networkinfo.CONNECTION_CELL2G:
										wifi(indexre, wodere, xiaoxire, comre);　　　
										break;　　　　
									case plus.networkinfo.CONNECTION_CELL3G:
