<script>
	
	
	function tiaozhuan(){
		var news_content=document.querySelector('#news_content');
		
		mui.openWindow({
			url:'/app/tongzhi/notice.html',
			id:'app/tongzhi/notice.html' 
		})
		
	}
	function bangzhu(t){
		
		
			mui.openWindow({
				url:'/app/tongzhi/bangzhuzhinan.html',
				id:'app/tongzhi/bangzhuzhinan.html' 
			})
		
		
		
	}
	function mianshi(){
		mui.openWindow({
			url:'/app/tongzhi/mianshi.html',
			id:'app/tongzhi/mianshi.html' 
		})
		
	}
	mui.plusReady(function(){
				document.body.querySelector("#qq").addEventListener("tap",function(){
					if (plus.os.name == "Android") { 
			            var main = plus.android.runtimeMainActivity(); 
			            var Intent = plus.android.importClass('android.content.Intent'); 
			            var Uri = plus.android.importClass('android.net.Uri'); 
			            var intent = new Intent(Intent.ACTION_VIEW, Uri.parse("mqqwpa://im/chat?chat_type=wpa&uin=1743276687")); 
			            main.startActivity(intent); 
			       } 
			       
			       if (plus.os.name == "iOS") {  
                       plus.runtime.launchApplication({  
                           action: "mqq://im/chat?chat_type=wpa&uin=1743276687&version=1&src_type=web"  
                       }, function(e) {  
                           plus.nativeUI.confirm("检查到您未安装qq，请先到appstore搜索下载？", function(i) {  
                               if (i.index == 0) {  
                                   iosAppstore("itunes.apple.com/cn/app/mqq/");  
                               }  
                           });  
                       });  
                   }  
				
		 		})
	});
</script>

</body>
