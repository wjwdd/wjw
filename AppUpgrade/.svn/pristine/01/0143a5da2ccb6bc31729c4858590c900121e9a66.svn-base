					<div class="mui-scroll sx" style="padding:10px;">
						<div class="sx" style="overflow: hidden;" ng-if="sx==1">
							<span ng-click="sxtj(this)" ng-click='sxbtn1(this)' data-bx="bx" class="sxbtn" style="display: inline-block;">不限</span><span ng-click="sxtj(this)" class="sxbtn" style="display: inline-block;" ng-repeat="x in quyu" ng-bind='x.categoryname'></span>

						</div>
						<div class="sx" style="overflow: hidden;" ng-if="sx==2">
							<span ng-click="sxzw(this)" class="sxbtn" style="display: inline-block;">不限</span><span ng-click="sxzw(this)" class="sxbtn" style="display: inline-block;" ng-repeat="x in zhiwei" ng-bind='x.categoryname'></span>

						</div>
						<div class="sx" style="overflow: hidden;" ng-if="sx==3">
							<span ng-click="sxxz(this)" class="sxbtn" style="display: inline-block;">不限</span><span ng-click="sxxz(this)" class="sxbtn" style="display: inline-block;" ng-repeat="x in xinzi" ng-bind='x.c_name'></span>

						</div>
						<div class="sx" style="overflow: hidden;" ng-if="sx==4">
							<span ng-click="sxfl(this)" class="sxbtn" style="display: inline-block;">不限</span><span ng-click="sxfl(this)" class="sxbtn" ng-repeat="x in fuli" ng-bind='x.c_name'></span>

						</div>
					</div>
				</div>
			</div>
		</div>
		<script src="js/mui.min.js"></script>
		<script src="js/ag1.46.js" type="text/javascript" charset="utf-8"></script>
		<script src="js/factory.js" type="text/javascript" charset="utf-8"></script>
		<script src="js/mui.pullToRefresh.js"></script>
		<script src="js/mui.pullToRefresh.material.js"></script>
		<script>
			mui.init({})
			angular.module('indexCtr', ['myApp'])
				.controller('myCtrl',
					function($scope, $http, $data) {

						var page = 1;
						var did = '';
						var zid = '';
						var xid = '';
						var fid = '';
						(function($) {
							//阻尼系数
							var deceleration = mui.os.ios ? 0.003 : 0.0009;
							$('.mui-scroll-wrapper').scroll({
								bounce: false,
								indicators: true, //是否显示滚动条
								deceleration: deceleration,
								scrollY: true, //是否竖向滚动
								scrollX: true, //是否横向滚动
								startX: 0, //初始化时滚动至x
								startY: -0, //初始化时滚动至y
							});
							var end = false;
							$.ready(function() {
								//循环初始化所有下拉刷新，上拉加载。
								$.each(document.querySelectorAll('.mui-slider-group .mui-scroll'), function(index, pullRefreshEl) {
									$(pullRefreshEl).pullToRefresh({
										down: {
											callback: function() {
												var self = this;
												setTimeout(function() {

													page = 1;
													xr(page);
													self.endPullDownToRefresh();
												}, 1500);
											}
										},
										up: {
											callback: function() {
												var self = this;

												setTimeout(function() {

													page++;
													$data.indexdata(page).success(function(response) {
														console.log(JSON.stringify(response.data.length));

														var data = response.data;
														if(data.length == 0) end = true;

														for(var i = 0; i < data.length; i++) {
															$scope.data.push(data[i]);
														}

														//						$scope.tag=response.data.tag;
														//						console.log(response.data.tag);
														//						console.log($scope.classname2)
													});
													console.log(end);
													self.endPullUpToRefresh(end);
												}, 1000);
											}
										}
									});
								});

							});
						})(mui);
						window.addEventListener('lisindex', function(e) {
							//在父页面中添加监听事件，刷新页面
							console.log(11);

							var cityid = localStorage.getItem('cityid');
							$data.indexquyu(cityid).success(function(response) {
								//console.log(JSON.stringify(response));
								$scope.city = localStorage.getItem('cityName');
								$scope.quyu = response.data;

							});

						});
						//职场分类
						mui(document.body).on('tap', '.zhichang', function() {

							//					alert(this.x.jid);
							mui.openWindow({
								url: 'app/circle/zhichang/zhichang.html',
								id: 'zhichang.html',
								createNew: false, //是否重复创建同样id的webview，默认为false:不重复创建，直接显示
								waiting: {
									autoShow: false, //自动显示等待框，默认为true
								}

							})
						})
						//名企推荐
						mui(document.body).on('tap', '.mingqi', function() {

							//					alert(this.x.jid);
							mui.openWindow({
								url: 'app/circle/mingqi/famousenterprise.html',
								id: 'famousenterprise.html',
								createNew: false, //是否重复创建同样id的webview，默认为false:不重复创建，直接显示
								waiting: {
									autoShow: false, //自动显示等待框，默认为true
								}

							})
						})
						//附近职位
						mui(document.body).on('tap', '.fujin', function() {

							//					alert(this.x.jid);
							mui.openWindow({
								url: 'app/circle/fujin/fujin.html',
								id: 'fujin.html',
								createNew: false, //是否重复创建同样id的webview，默认为false:不重复创建，直接显示
								waiting: {
									autoShow: false, //自动显示等待框，默认为true
								}

							})
						})
						//快速入职
						mui(document.body).on('tap', '.kuaisu', function() {

							//					alert(this.x.jid);
							mui.openWindow({
								url: 'app/circle/kuaisu/kuaisu.html',
								id: 'kuaisu.html',
								createNew: false, //是否重复创建同样id的webview，默认为false:不重复创建，直接显示
								waiting: {
									autoShow: false, //自动显示等待框，默认为true
								}

							})
						})
						//搜索
						mui(document.body).on('tap', '.selectcity', function() {

							//					alert(this.x.jid);
							mui.openWindow({
								url: 'app/citylist/citylist.html',
								id: 'citylist.html',
								createNew: false, //是否重复创建同样id的webview，默认为false:不重复创建，直接显示
								waiting: {
									autoShow: false, //自动显示等待框，默认为true
								}

							})
						})
						//免费招聘
						mui(document.body).on('tap', '.mfzp', function() {

							//					alert(this.x.jid);
							mui.openWindow({
								url: 'app/zhaopin/zhao.html',
								id: 'zhao.html',
								createNew: false, //是否重复创建同样id的webview，默认为false:不重复创建，直接显示
								waiting: {
									autoShow: false, //自动显示等待框，默认为true
								}
							})
						})
						//城市选择
						mui(document.body).on('tap', '.search', function() {

							//					alert(this.x.jid);
							mui.openWindow({
								url: 'app/search/search.html',
								id: 'search.html',
								createNew: false, //是否重复创建同样id的webview，默认为false:不重复创建，直接显示
								waiting: {
									autoShow: false, //自动显示等待框，默认为true
								}
							})
						})
						//职位详情
						mui('.mui-table-view').on('tap', '.jobdet', function() {

							var jid = this.getAttribute('data-id');

							localStorage.setItem('jid', jid);
							//					alert(this.x.jid);
							mui.openWindow({
								url: 'app/job/job.html',
								id: 'job',
								createNew: false, //是否重复创建同样id的webview，默认为false:不重复创建，直接显示
								waiting: {
									autoShow: false, //自动显示等待框，默认为true
								}

							})
						})

						//渲染
						xr(page);

						function xr(page) {
							$data.indexdata(page).success(function(response) {
								//							console.log(JSON.stringify(response));
								$scope.data = response.data;
								$scope.imgurl = $$$.imgurl;
								$scope.selbox = false;
								$scope.sxquyu = '区域';
								$scope.sxzhiwei = '职位';
								$scope.sxxinzi = '薪资';
								$scope.sxfuli = '福利';
								$scope.city = localStorage.getItem('cityName');
								if(localStorage.getItem('cityid') == null) {
									$scope.city = '青岛';
									localStorage.setItem('cityid', 805);
								}
							});
						}

						mui('#selector').on('tap', '.xian', function() {
							var sid = this.getAttribute('id');
							if(sid == 'selector1') {
								$scope.sx = 1;
								var cityid = localStorage.getItem('cityid');
								$data.indexquyu(cityid).success(function(response) {
									//																console.log(JSON.stringify(response));
									$scope.quyu = response.data;

								});

							} else if(sid == 'selector2') {
								$scope.sx = 2;
								$data.indexzhiwei().success(function(response) {
									//																console.log(JSON.stringify(response));
									$scope.zhiwei = response.zhiwei;
									//								console.log(response.zhiwei.length);
									//								for(var i=0;i<response.zhiwei.length;i++){
									//									console.log(response.zhiwei[i].categoryname);
									//								}

								});
							} else if(sid == 'selector3') {
								$scope.sx = 3;
								$data.indexxinzi().success(function(response) {
									//								console.log(JSON.stringify(response));
									$scope.xinzi = response.wge;

									//								console.log(JSON.stringify(response.wge[1].c_name));

								});
							} else if(sid == 'selector4') {
								$scope.sx = 4;
								$data.indexfuli().success(function(response) {
									//								console.log(JSON.stringify(response));
									$scope.fuli = response.welfare;

								});
							}
							$scope.selbox = !$scope.selbox;
							mui("#popover").popover("toggle");
						})
						//筛选的点击事件
						$scope.sxtj = function() {
							//						console.log(this.x.categoryname);
							if(this.x == undefined) {
								$scope.sxquyu = '区域';
								did = '';
							} else {
								$scope.sxquyu = this.x.categoryname;
								localStorage.setItem('sel_did', this.x.categoryname);
								did = '&dist=' + localStorage.getItem('sel_did');
								console.log(did);

							}

							$data.indexshaixuan(did, zid, xid, fid).success(function(response) {
								console.log(JSON.stringify(response));
								$scope.data = response.data;
							})
							mui("#popover").popover("toggle");
						}
						$scope.sxzw = function() {
							//						console.log(this.x.categoryname);
							if(this.x == undefined) {
								$scope.sxzhiwei = '职位';
								zid = '';
							} else {
								$scope.sxzhiwei = this.x.categoryname;
								localStorage.setItem('sel_zid', this.x.categoryname);
								zid = '&job=' + localStorage.getItem('sel_zid');
								console.log(zid);

							}
							$data.indexshaixuan(did, zid, xid, fid).success(function(response) {
								console.log(JSON.stringify(response));
								$scope.data = response.data;
							})
							mui("#popover").popover("toggle");
						}
						$scope.sxxz = function() {
							//						console.log(this.x.categoryname);
							if(this.x == undefined) {
								$scope.sxxinzi = '薪资';
								xid = '';
							} else {
								$scope.sxxinzi = this.x.c_name;
								localStorage.setItem('sel_xid', this.x.c_name);
								var regxid = localStorage.getItem('sel_xid');
								var re = /([0-9]+)([0-9]+)/g;
								min = regxid.match(re)[0];
								max = regxid.match(re)[1];
								console.log(min);
								xid = '&min=' + min + '&max=' + max;
								//							console.log(did);
								//							console.log(zid);
								//							console.log(xid);

							}
							$data.indexshaixuan(did, zid, xid, fid).success(function(response) {
								console.log(JSON.stringify(response));
								$scope.data = response.data;
							})
							mui("#popover").popover("toggle");
						}
						$scope.sxfl = function() {
							//						console.log(this.x.categoryname);
							if(this.x == undefined) {
								$scope.sxfuli = '福利';
								fid = '';
							} else {
								$scope.sxfuli = this.x.c_name;
								localStorage.setItem('sel_fid', this.x.c_name);
								fid = '&welfare=' + localStorage.getItem('sel_fid');
								console.log(fid);

							}
							$data.indexshaixuan(did, zid, xid, fid).success(function(response) {
								console.log(JSON.stringify(response));
								$scope.data = response.data;
							})
							mui("#popover").popover("toggle");
						}
						//轮播图
						$data.indexlunbo().success(function(response) {
							console.log(JSON.stringify(response));
							$scope.lunbo = response.data;
							//						$scope.tag=response.data.tag;
							//						console.log(response.data.tag);
							//						console.log($scope.classname2)
						});
						mui.plusReady(function() {
							//获得slider插件对象
							var gallery = mui('.mui-slider');
							gallery.slider({
								interval: 3000 //自动轮播周期，若为0则不自动播放，默认为0；
							});
						});
						

					})

			//cjs

			if(window.plus) {
				plusReady();
			} else {
				document.addEventListener('plusready', plusReady, false);
			}

			function plusReady() {

			}

			
		</script>
	</body>

</html>