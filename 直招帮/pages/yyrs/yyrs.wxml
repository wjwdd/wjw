<!--pages/yyrs/yyrs.wxml-->
<view class="weui-cells weui-cells_after-title">
  <view wx:for="{{yqlist}}" wx:key="{{yqkey}}" class="weui-cell">
    <view class="weui-cell__hd">
      <image src="{{imgjoburl+item.img}}" style="margin-right: 5px;vertical-align: middle;width:30px; height: 30px;"></image>
    </view>
    <view class="weui-cell__bd">{{item.fullname}}</view>
    <view class="weui-cell__bd" style='-webkit-box-flex:2;-webkit-flex:2;flex:2'>{{item.mobile}}</view>
    <view class="weui-cell__ft {{item.state==1?'rzgreen':'wrzred'}}">{{item.state==1?'入职':'未入职'}}</view>
  </view>

</view>
<view hidden='{{hidden}}' class="weui-loadmore weui-loadmore_line">
  <view class="weui-loadmore__tips weui-loadmore__tips_in-line">暂无数据</view>
</view>